{
  "family": "clothesshop-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"], 
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::545009863780:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "clothesshop",
          "image": "545009863780.dkr.ecr.us-east-1.amazonaws.com/website-clothesshop-server:latest",
          "essential": true,
          "portMappings": [
              {
                  "containerPort": 3000,
                  "hostPort": 3000
              }
          ]
      }
  ]
}

  